{
  "name": "SpotMobile",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "lint": "eslint --ext .js App",
    "lint:fix": "npm run lint -- --fix",
    "clean": "rm -rf $TMPDIR/react-* && npm cache clean --force",
    "android": "cd ./android && ./gradlew app:assembleDebug && ./gradlew installDebug",
    "clean:android": "cd android/ && ./gradlew clean && cd .. && react-native run-android",
    "release:android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output ./android/app/src/main/assets/index.android.bundle --assets-dest ./android/app/src/main/res/ && rm -rf ./android/app/src/main/res/drawable-mdpi && rm -rf ./android/app/src/main/res/drawable-xhdpi && rm -rf ./android/app/src/main/res/drawable-xxhdpi && cd android && ./gradlew clean && ./gradlew assembleRelease && cd ..",
    "release:install:android": "adb -s 88HKBN322EEW install ./android/app/build/outputs/apk/release/app-release.apk",
    "newclear": "rm -rf $TMPDIR/react-* && watchman watch-del-all && rm -rf ios/build && rm -rf node_modules/ && npm cache clean --force && npm i",
    "test": "jest",
    "android:emulator": "cd $ANDROID_HOME/tools && ./emulator -avd react-native",
    "precommit": "npm run lint -s",
    "android-bundle": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res && react-native run-android",
    "postinstall": "./node_modules/react-native-maps/enable-google-maps 'ios/Pods/GoogleMaps/**'",
    "reload:android": "adb shell input keyevent KEYCODE_MENU && adb shell input keyevent ENTER && adb shell input keyevent ENTER"
  },
